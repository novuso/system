{
    "name": "novuso/system",
    "description": "System Library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.6",
        "overtrue/phplint": "^9.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.9",
        "symfony/finder": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\System\\": "src",
            "Novuso\\System\\Test\\TestCase\\": "tests/TestCase"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\System\\Test\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.16"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
