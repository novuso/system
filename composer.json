{
    "name": "novuso/system",
    "description": "Novuso System Library",
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/finder": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\System\\": "src",
            "Novuso\\System\\Test\\TestCase\\": "tests/TestCase"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\System\\Test\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
